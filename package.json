    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@vueuse/core": "^11.2.0",
    "quill": "^2.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@directus/extensions-sdk": "12.1.2",
    "eslint": "^9.14.0",
    "typescript": "^5.6.3",
    "vue": "^3.5.12"
  }
